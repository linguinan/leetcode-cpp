{
    "files.associations": {
        "*.command": "shellscript",
        "map": "cpp",
        "__tree": "cpp",
        "type_traits": "cpp",
        "__hash_table": "cpp",
        "__split_buffer": "cpp",
        "array": "cpp",
        "bitset": "cpp",
        "initializer_list": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "unordered_map": "cpp",
        "utility": "cpp",
        "vector": "cpp",
        "iosfwd": "cpp",
        "iterator": "cpp",
        "chrono": "cpp",
        "cstring": "cpp",
        "queue": "cpp",
        "set": "cpp",
        "algorithm": "cpp",
        "unordered_set": "cpp",
        "typeinfo": "cpp",
        "functional": "cpp"
    },
    // "C_Cpp.default.cppStandard": "c++11"
    "clang.cxxflags": ["-std=c++14"],
    "code-runner.executorMap": {
        "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt -std=c++11 && $dir$fileNameWithoutExt"
    },
}